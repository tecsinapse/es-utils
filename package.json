{
  "name": "@tecsinapse/es-utils",
  "version": "0.0.94",
  "description": "A Utils library to turn life easy for Javascript projects",
  "main": "core/index.js",
  "author": "Tecsinapse",
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "lodash.uniq": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@tecsinapse/eslint-config-tecsinapse-js": "^2.1.2",
    "babel-plugin-emotion": "^10.0.5",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "husky": "^1.3.1",
    "lint-staged": "7.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "peerDependencies": {
    "raven-js": "^3.27.0"
  },
  "scripts": {
    "build": "rm -rf build/ && babel src/ -d core/",
    "dev": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm publish --access public",
    "docz:dev": "docz dev",
    "docz:build": "docz build --base='/es-utils/'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tecsinapse/es-utils.git"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "core"
  ]
}
